{
    "Labels": {
        "type": "object",
        "additionalProperties": {
            "$ref": "./schemas.json#/Term"
        },
        "example": {
            "en": {
                "language": "en",
                "value": "Douglas Adams"
            },
            "fr": {
                "language": "fr",
                "value": "Douglas Adams"
            }
        }
    },
    "Descriptions": {
        "type": "object",
        "additionalProperties": {
            "$ref": "./schemas.json#/Term"
        },
        "example": {
            "en": {
                "language": "en",
                "value": "Science-fiction writer"
            },
            "fr": {
                "language": "fr",
                "value": "écrivain de science-fiction"
            }
        }
    },
    "Aliases": {
        "type": "object",
        "additionalProperties": {
            "$ref": "./payloads.json#/AliasList"
        },
        "example": {
            "en": [
                {
                    "language": "en",
                    "value": "Douglas Noel Adams"
                },
                {
                    "language": "en",
                    "value": "Douglas Noël Adams"
                }
            ],
            "fr": [
                {
                    "language": "fr",
                    "value": "Douglas Noel Adams"
                }
            ]
        }
    },
    "Item": {
        "allOf": [
            {
                "$ref": "./schemas.json#/Entity"
            },
            {
                "$ref": "./interfaces.json#/Fingerprintable"
            },
            {
                "$ref": "./interfaces.json#/StatementsBearing"
            },
            {
                "type": "object",
                "properties": {
                    "sitelinks": { "$ref": "./interfaces.json#/SitelinkMap" }
                }
            }
        ]
    },
    "Property": {
        "allOf": [
            {
                "$ref": "./schemas.json#/Entity"
            },
            {
                "$ref": "./interfaces.json#/Fingerprintable"
            },
            {
                "$ref": "./interfaces.json#/StatementsBearing"
            },
            {
                "type": "object",
                "properties": {
                    "datatype": {
                        "type": "string"
                    }
                },
                "required": [
                    "datatype"
                ]
            }
        ]
    },
    "Entity": {
        "type": "object",
        "properties": {
            "id": {
                "type": "string"
            },
            "type": {
                "type": "string"
            },
            "modified": {
                "type": "string",
                "format": "date-time"
            }
        },
		"required": [
            "id",
            "type"
        ],
        "discriminator": {
            "propertyName": "type"
        }
    },
    "Term": {
        "type": "object",
        "properties": {
            "language": {
                "type": "string"
            },
            "value": {
                "type": "string"
            }
        },
        "required": [
            "language",
            "value"
        ]
    },
    "QualifierHash": {
        "description": "Hash of a qualifier",
        "type": "string",
        "example": "455481eeac76e6a8af71a6b493c073d54788e7e9"
    },
    "ReferenceHash": {
        "description": "Hash of a reference",
        "type": "string",
        "example": "455481eeac76e6a8af71a6b493c073d54788e7e9"
    },
    "Statement": {
        "type": "object",
        "properties": {
            "mainsnak" : {
                "$ref": "./interfaces.json#/Snak"
            },
            "id": {
                "type": "string"
            },
            "rank": {
                "type": "string"
            },
            "qualifiers": {
                "$ref": "./schemas.json#/Qualifiers"
            },
            "references": {
                "$ref": "./schemas.json#/References"
            }
        }
    },
    "Sitelink": {
        "type": "object",
        "properties": {
            "site": {
                "type": "string"
            },
            "title": {
                "type": "string"
            },
            "badges": {
                "type": "array",
                "items": { "type": "string" }
            }
        }
    },
    "Qualifier": {
        "$ref": "./interfaces.json#/Snak"
    },
    "Qualifiers": {
        "$ref": "./interfaces.json#/SnakMap"
    },
    "Reference": {
        "type": "object",
        "properties": {
            "snaks": {
                "$ref": "./interfaces.json#/SnakMap"
            }
        }
    },
    "References": {
        "type": "array",
        "items": { "$ref": "./schemas.json#/Reference" }
    },
    "PersistedStatement": {
        "type": "object",
        "properties": {
            "mainsnak" : {
                "$ref": "./schemas.json#/HashedSnak"
            },
            "id": {
                "type": "string"
            },
            "rank": {
                "type": "string"
            },
            "qualifiers": {
                "$ref": "./schemas.json#/PersistedQualifiers"
            },
            "references": {
                "$ref": "./schemas.json#/PersistedReferences"
            }
        }
    },
    "PersistedReference": {
        "allOf": [
            {
                "$ref": "./schemas.json#/Reference"
            },
            {
                "type": "object",
                "properties": { "hash": { "$ref": "./schemas.json#/ReferenceHash" } }
            }
        ],
        "example": {
            "hash": "",
            "snaks": {
                "P31": [],
                "P100": []
            }
        }
    },
    "PersistedReferences": {
        "type": "array",
        "items": { "$ref": "./schemas.json#/PersistedReference" }
    },
    "PersistedQualifiers": {
        "$ref": "./schemas.json#/HashedSnakMap"
    },
    "HashedSnak": {
        "allOf": [
            {
                "$ref": "./interfaces.json#/Snak"
            },
            {
                "type": "object",
                "properties": { "hash": { "$ref": "./interfaces.json#/SnakHash" } }
            }
        ]
    },
    "HashedSnakList": {
        "type": "array",
        "items": { "$ref": "./schemas.json#/HashedSnak" }
    },
    "HashedSnakMap": {
        "type": "object",
        "additionalProperties": {
            "$ref": "./schemas.json#/HashedSnakList"
        },
        "example": {
            "P31": [],
            "P100": []
        }
    }
}