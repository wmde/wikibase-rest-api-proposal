    "@apidevtools/swagger-parser": "^10.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^4.3.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.4.1",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
