{
    "get": {
        "tags": [ "properties" ],
        "summary": "Retrieves an entity's aliases list",
        "parameters": [
            { "$ref": "../../../../parameters.json#/offset" },
            { "$ref": "../../../../parameters.json#/page" },
            { "$ref": "../../../../parameters.json#/per_page" }
        ],
        "responses": {
            "200": { "$ref": "../../../../responses.json#/EntityAliasesList" },
            "404": { "$ref": "../../../../responses.json#/NotFound" },
            "default": { "$ref": "../../../../responses.json#/UnexpectedError" }
        }
    },
    "put": {
        "tags": ["properties"],
        "summary": "Set/Replace aliases in multiple languages at once",
        "parameters": [
            { "$ref": "../../../../parameters.json#/entityId" }
        ],
        "requestBody": { "$ref": "../../../../requests.json#/Property" },
        "responses": {
            "200": { 
                "$ref": "../../../../responses.json#/EntityAliasesList"
             },
            "404": { "$ref": "../../../../responses.json#/NotFound" },
            "default": { "$ref": "../../../../responses.json#/UnexpectedError" }
        }
    }
}