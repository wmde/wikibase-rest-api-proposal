{
    "get": {
        "tags": [ "items" ],
        "summary": "Retrieves an Item's sitelink by wiki-id",
        "parameters": [
            { "$ref": "../../../../parameters.json#/entityId" },
            { "$ref": "../../../../parameters.json#/wikiId" }
        ],
        "responses": {
            "200": { "$ref": "../../../../responses.json#/Sitelink" },
            "404": { "$ref": "../../../../responses.json#/NotFound" },
            "default": { "$ref": "../../../../responses.json#/UnexpectedError" }
        }
    },
    "patch": {
        "tags": [ "items" ],
        "summary": "Updates an Item's sitelink by wiki-id",
        "parameters": [
            { "$ref": "../../../../parameters.json#/entityId" },
            { "$ref": "../../../../parameters.json#/wikiId" }
        ],
        "requestBody": { "$ref": "../../../../requests.json#/PatchList" },
        "responses": {
            "200": { "$ref": "../../../../responses.json#/Sitelink" },
            "404": { "$ref": "../../../../responses.json#/NotFound" },
            "default": { "$ref": "../../../../responses.json#/UnexpectedError" }
        }
    }
}