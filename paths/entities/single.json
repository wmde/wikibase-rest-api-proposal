{
    "get": {
        "tags": [ "entities" ],
        "summary": "Single Entity",
        "parameters": [
            { "$ref": "../../parameters.json#/entityType" },
            { "$ref": "../../parameters.json#/entityId" },
            { "$ref": "../../parameters.json#/fields" }
        ],
        "responses": {
            "200": { "$ref": "../../responses.json#/Entity" },
            "404": { "$ref": "../../responses.json#/NotFound" },
            "default": { "$ref": "../../responses.json#/UnexpectedError" }
        }
    },
    "put": {
        "tags": ["entities"],
        "summary": "Replace a Wikibase Entity by Id",
        "parameters": [
            { "$ref": "../../parameters.json#/entityType" },
            { "$ref": "../../parameters.json#/entityId" }
        ],
        "requestBody": { "$ref": "../../requests.json#/Entity" },
        "responses": {
            "200": { 
                "description": "Wikibase Entity ID",
                "content": {
                    "application/json": {
                        "schema": { "type": "string" }
                    }
                }
             },
            "404": { "$ref": "../../responses.json#/NotFound" },
            "default": { "$ref": "../../responses.json#/UnexpectedError" }
        }
    },
    "patch": {
        "tags": ["entities"],
        "summary": "Update a Wikibase Entity by Id",
        "parameters": [
            { "$ref": "../../parameters.json#/entityType" },
            { "$ref": "../../parameters.json#/entityId" }
        ],
        "requestBody": { "$ref": "../../requests.json#/PatchList" },
        "responses": {
            "200": { "$ref": "../../responses.json#/Entity" },
            "404": { "$ref": "../../responses.json#/NotFound" },
            "default": { "$ref": "../../responses.json#/UnexpectedError" }
        }
    },
    "delete": {
        "tags": [ "entities" ],
        "summary": "Single Entity",
        "parameters": [
            { "$ref": "../../parameters.json#/entityType" },
            { "$ref": "../../parameters.json#/entityId" }
        ],
        "responses": {
            "200": { "$ref": "../../responses.json#/OK" },
            "404": { "$ref": "../../responses.json#/NotFound" },
            "default": { "$ref": "../../responses.json#/UnexpectedError" }
        }
    }
}